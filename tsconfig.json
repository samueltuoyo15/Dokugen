{
  "compilerOptions": {
    "target": "es2022",
    "module": "NodeNext",
    //"rootDir": ".",
    "allowJs": true,
    "moduleResolution": "NodeNext",
    "outDir": "dist",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
